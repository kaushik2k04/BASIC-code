import java.util.Scanner;

public class evenodd {
    public static void main(String[] args) {
        int a;
        Scanner scr =new Scanner(System.in);
        a=scr.nextInt();
        if(a%2==0){
            System.out.println("EVEN");
        }
        else{
            System.out.println();
        }
    }
    
}
